<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".obsever.ObserverActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_on_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="On Next"/>

    <Button
        android:id="@+id/btn_on_completed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="On Completed"/>

    <Button
        android:id="@+id/btn_on_error"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="On Error"/>

    <Button
        android:id="@+id/btn_on_dispose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Disposable"/>


    <Button
        android:id="@+id/btn_subscriber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subscriber"/>



</LinearLayout>